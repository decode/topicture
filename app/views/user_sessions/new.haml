%h1 Login

- form_for @user_session, :url => user_session_path do |f|
  = #f.error_messages
  #login_container
    = f.label :login
    = f.text_field :login
    %br
    = f.label :password
    = f.password_field :password
    %br
    = f.check_box :remember_me
    = f.label :remember_me
    %br

    = f.submit "Login"

